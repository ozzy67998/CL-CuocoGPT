ex:Recipe_100002_Step_1 a ex:Step ;
    rdfs:comment "crush 16 cookies in a sealable plastic bag until fine crumbs form" .

ex:Recipe_100002_Step_10 a ex:Step ;
    rdfs:comment "top with reserved cookie crumbs" .

ex:Recipe_100002_Step_2 a ex:Step ;
    rdfs:comment "mix 1 1 / 2 cups of the crumbs with melted butter" .

ex:Recipe_100002_Step_3 a ex:Step ;
    rdfs:comment "press firmly onto bottom and up side of 9-inch pie plate to form crust" .

ex:Recipe_100002_Step_4 a ex:Step ;
    rdfs:comment "reserve remaining crumbs" .

ex:Recipe_100002_Step_5 a ex:Step ;
    rdfs:comment "combine 1 1 / 2 cups milk and pudding mix in medium bowl" .

ex:Recipe_100002_Step_6 a ex:Step ;
    rdfs:comment "stir for 2 minutes with wire whisk" .

ex:Recipe_100002_Step_7 a ex:Step ;
    rdfs:comment "gently stir in 2 cups thawed cool whip" .

ex:Recipe_100002_Step_8 a ex:Step ;
    rdfs:comment "spoon into crust" .

ex:Recipe_100002_Step_9 a ex:Step ;
    rdfs:comment "refrigerate 30 minutes or until set" .

